<%= form_tag user_sessions_path, :method => :post do %>
  <table>
    <tr>
      <td><%= label_tag :Benutzername %></td>
      <td><%= text_field_tag :username %></td>
    </tr>
    <tr>
      <td><%= label_tag :Passwort %></td>
      <td><%= password_field_tag :password %></td>
    </tr>
  </table>
  
  <br>
  <%= submit_tag "anmelden" %>
<% end %>
